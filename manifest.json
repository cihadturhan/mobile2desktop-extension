{
  "manifest_version": 2,
  "name": "Mobile2Desktop",
  "description": "Mobile to desktop, desktop to mobile fast data transfer",
  "version": "1.0",
  "browser_action": {
    "name": "Click to show content.",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": [
      "socket.io.js",
      "moment.min.js",
      "background.js"
    ],
    "persistent": true
  }
}
